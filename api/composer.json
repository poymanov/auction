{
    "name": "poymanov/auction",
    "type": "project",
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "ext-json": "^7.4",
        "php-di/php-di": "^6.1",
        "slim/psr7": "^1.0",
        "slim/slim": "^4.5",
        "symfony/console": "^5.0"
    },
    "require-dev": {
        "overtrue/phplint": "^2.0",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "lint": "phplint",
        "app": "php bin/app.php --ansi"
    }
}
